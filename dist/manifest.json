{
  "manifest_version": 3,
  "name": "Linkrew Extension",
  "version": "1.0.0",
  "description": "웹페이지를 Linkrew에 저장하세요",
  "action": {
    "default_popup": "extension.html",
    "default_icon": {
      "16": "LinkrewForExtension.png",
      "48": "LinkrewForExtension.png",
      "128": "LinkrewForExtension.png"
    }
  },
  "permissions": ["tabs", "activeTab"],
  "host_permissions": ["http://localhost:8080/*"]
}
